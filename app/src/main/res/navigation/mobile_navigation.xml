<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.blockbasti.cragtrackapp.ui.menu.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_menu_dashboard" />

    <fragment
        android:id="@+id/navigation_sessions"
        android:name="com.blockbasti.cragtrackapp.ui.menu.sessions.SessionsFragment"
        android:label="@string/title_sessions"
        tools:layout="@layout/fragment_menu_sessions" />

    <fragment
        android:id="@+id/navigation_statistics"
        android:name="com.blockbasti.cragtrackapp.ui.menu.statistics.StatisticsFragment"
        android:label="@string/title_statistics"
        tools:layout="@layout/fragment_menu_statistics" />
</navigation>
